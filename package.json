{
  "name": "nonspace-ui",
  "version": "0.1.0",
  "description": "Shared Vue 3 UI components for nonspace and ecos projects",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "style": "./dist/style.css"
    },
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css",
    "./components/*": {
      "types": "./dist/components/*.vue.d.ts",
      "import": "./src/components/*.vue"
    },
    "./composables/*": {
      "types": "./dist/composables/*.d.ts",
      "import": "./src/composables/*.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vue-tsc --declaration --emitDeclarationOnly --outDir dist && vite build",
    "dev": "vite build --watch",
    "type-check": "vue-tsc --noEmit",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "typescript": "^5.5.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.0",
    "vue": "^3.5.0",
    "vue-tsc": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slug23/nonspace-ui.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "image-viewer",
    "lightbox"
  ],
  "license": "MIT"
}



